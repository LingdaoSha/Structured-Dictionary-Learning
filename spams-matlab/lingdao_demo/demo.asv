function demo

clear all;
addpath('./images/');

im = imread('im1.tif');

for i =1:3
   m(:,i) = reshape(im(:,:,i), [], 1);
end

m = normc(double(m)); % normc(M) normalizes the columns of M to a length of 1

m = center(m);

[U D V] = svd(double(m), 'econ');

param.K=3;  % learns a dictionary with 64 elements
param.lambda=0.05;
param.numThreads=4; % number of threads
param.batchsize=400;
param.tol = 1e-3
param.iter=200;

